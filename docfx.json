{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**/*.csproj"
          ],
          "cwd": ".",
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "_site/**",
            "**/Docs/**",
            "**/APIDocs/**",
            "**/Tests/**"
          ],
          "src": "../"
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml"
        ],
        "cwd": "obj"
      },
      {
        "files": [
          "APIDocs/**.md",
          "Docs/**.md",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/Assets/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "API/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "globalMetadata": {
      "_appTitle": "Devdog.io asset documentation",
      "_appFooter": "",
      "_enableSearch": true,
      "_disableContribution": true
    },
    "fileMetadata": {
      "priority": {
        "obj/docfx/**": 1.0,
        "**.md": 2.5,
        "APIDocs/**.md": 2.5,
        "Docs/**.md": 2.5,
        "API/**.md": 2
      },
      "keywords": {
        "obj/docfx/**": [ "API", "Reference" ],
        "APIDocs/**.md": [ "API", "API Documentation", "Documentation" ],
        "Docs/**.md": [ "Documentation", "Tutorials", "Guides" ],
        "API/**.md": [ "API", "Script Reference", "Documentation" ]
      }
    },
    "sitemap":{
      "baseUrl": "https://devdog.io",
      "priority": 0.1,
      "changefreq": "monthly",
      "fileOptions":{
        "**/API/**.yml": {
          "priority": 0.3,
          "lastmod": "2001-01-01"
        }
      }
    },
    "dest": "_site",
    "template": [
      "default",
      "Template"
    ]
  }
}